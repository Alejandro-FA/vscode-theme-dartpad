{
	"name": "DartPad Candy",
	"type": "dark",
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"parameter": {
			"bold": true,
			"italic": true
		},
		"keyword.void": "#ff906f",
		"source.interpolation": "#ffffff",
		"property.annotation": "#919090",
		"selfParameter": "#51e191",
		"variable.importPrefix": "#fb557e"
	},
	"tokenColors": [
		{
			"name": "Namespaces",
			"scope": [
				"entity.name.namespace",
				"storage.modifier.package",
				"storage.modifier.import"
			],
			"settings": {
				"foreground": "#fb557e"
			}
		},
		{
			"name": "Types and classes",
			"scope": [
				"entity.name.type",
				"storage.type.primitive",
				"storage.type.built-in.primitive",
				"support.type", // Types
				"storage.type.struct", // Structs
				"entity.name.type.class", // Classes
				"support.class", // Classes default library
				"entity.name.type.interface", // Interfaces
				"entity.name.type.enum" // Enums
			],
			"settings": {
				"foreground": "#ff906f"
			}
		},
		{
			"name": "Functions and methods",
			"scope": [
				"entity.name.function",
				"meta.function",
				"meta.function-call",
				"support.function",
				"entity.name.funcion.member" // Class methods
			],
			"settings": {
				"foreground": "#ff92d9"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable.other.readwrite",
				"entity.name.variable"
			],
			"settings": {
				"foreground": "#d5e9ed"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"variable.other.constant",
				"meta.conditional.case",
				"meta.parens"
			],
			"settings": {
				"foreground": "#0ad2fb"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric",
				"keyword.other.unit.suffix"
			],
			"settings": {
				"foreground": "#919090"
			}
		},
		{
			"name": "Constants from default library",
			"scope": [
				"support.constant",
				"constant.language"
			],
			"settings": {
				"foreground": "#ff906f"
			}
		},
		{
			"name": "Parameters",
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#0ad2fb"
			}
		},
		{
			"name": "Properties",
			"scope": [
				"variable.other.property",
				"meta.attribute",
				"support.type.property-name.json",
				"variable.other.enummember" // Enum members
			],
			"settings": {
				"foreground": "#0ad2fb"
			}
		},
		{
			"name": "Events",
			"scope": "variable.other.event",
			"settings": {
				// "foreground": "#FF0000",
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"string.quoted.docstring",
				"string.quoted.double.block"
			],
			"settings": {
				"foreground": "#919bc3"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"keyword.operator.new",
				"variable.language",
				"storage.modifier",
				"storage.type",
				"keyword.operator.logical.python"
			],
			"settings": {
				"foreground": "#51e191"
			}
		},
		{
			"name": "Language variables",
			"scope": "variable.language",
			"settings": {
				"fontStyle": "bold italic"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"entity.name.function.operator",
				"storage.modifier.reference.cpp"
			],
			"settings": {
				"foreground": "#51e191"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#fb557e"
			}
		},
		{
			"name": "Type annotations",
			"scope": "storage.type.annotation",
			"settings": {
				"foreground": "#919090"
			}
		},
		{
			"name": "Macros",
			"scope": [
				"entity.name.function.macro",
				"meta.preprocessor.macro"
			],
			"settings": {
				"foreground": "#0ad2fb"
			}
		},
		{
			"name": "Pragmas",
			"scope": "entity.other.attribute-name.pragma",
			"settings": {
				"foreground": "#919090"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.section",
				"punctuation.separator",
				"punctuation.definition.begin.bracket",
				"punctuation.definition.end.bracket",
				"punctuation.terminator",
				"punctuation.definition.parameters",
				"punctuation.definition.arguments",
				"punctuation.parenthesis",
				"punctuation.definition.dict",
				"storage.modifier.array.bracket",
				"punctuation.definition.template-expression",
				"meta.brace.square"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		/***************************** Markdown ******************************/
		{
			"name": "[VSCODE-CUSTOM] Markdown headings",
			"scope": [
				"entity.name.section.markdown",
				"punctuation.definition.heading.markdown",
				"markup.heading.setext"
			],
			"settings": {
				"foreground": "#00d2fb"
			}
		},
		{
			"name": "Punctuation definitions",
			"scope": [
				"punctuation.definition.list.begin.markdown",
				"punctuation.definition.list.markdown",
				"beginning.punctuation.definition.list.markdown",
				"punctuation.definition.metadata.markdown",
				"punctuation.definition.link",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#00d2fb"
			}
		},
		{
			"name": "[VSCODE-CUSTOM] Markdown Bold/Italic",
			"scope": [
				"punctuation.definition.bold.markdown",
				"punctuation.definition.italic",
				"punctuation.definition.math",
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#919090"
			}
		},
		{
			"name": "[VSCODE-CUSTOM] Markdown Code",
			"scope": [
				"markup.inline.raw.markdown",
				"markup.inline.raw.string.markdown",
				"markup.fenced_code"
			],
			"settings": {
				"foreground": "#51e191"
			}
		},
		{
			"name": "[VSCODE-CUSTOM] Markdown Underline Link/Image",
			"scope": [
				"markup.underline.link.markdown",
				"markup.underline.link.image.markdown"
			],
			"settings": {
				"foreground": "#fb557e"
			}
		},
		{
			"name": "[VSCODE-CUSTOM] Markdown Link Title/Description",
			"scope": [
				"string.other.link.title.markdown",
				"string.other.link.description.markdown"
			],
			"settings": {
				"foreground": "#00d2fb"
			}
		},
		{
			"name": "Html tags",
			"scope": "text.html.markdown",
			"settings": {
				"foreground": "#919090"
			}
		},
		{
			"name": "Paragrpah text",
			"scope": "meta.paragraph.markdown",
			"settings": {
				"foreground": "#eeeeee"
			}
		},
		{
			"name": "Math formulas",
			"scope": "meta.embedded.math.markdown",
			"settings": {
				"foreground": "#919090"
			}
		}
	],
